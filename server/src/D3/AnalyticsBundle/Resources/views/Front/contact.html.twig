
{#{% extends 'D3AnalyticsBundle::layout.html.twig' %}#}

{% block title %}Contact{% endblock%}

{% block body %}
    <header>
        <h1>Contact D3Analytics</h1>
    </header>


    <p>Want to contact d3?</p>

    <form action="{{ path('D3AnalyticsBundle_contact') }}" method="post" {{ form_enctype(form) }} class="d3">
        {{ form_errors(form) }}

        {{ form_row(form.name) }}
        {{ form_row(form.description) }}
        {{ form_row(form.visualizationTypeId) }}

        {{ form_rest(form) }}

        <input type="submit" value="Submit" />
    </form>

{% endblock %}